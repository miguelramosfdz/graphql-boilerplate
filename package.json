{
  "name": "graphql-basics",
  "version": "1.0.0",
  "description": "Graphql Play around",
  "main": "index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "ts-node": "ts-node src/index.ts",
    "dev": "env-cmd ./config/dev.env nodemon",
    "debug": "env-cmd ./config/dev.env nodemon --exec ts-node --inspect --debug-brk src/index.ts",
    "serve": "env-cmd ./config/dev.env node dist/index.js",
    "start": "node dist/index.js",
    "build": "tsc --build tsconfig.json",
    "heroku-postbuild": "yarn build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-modules/core": "0.7.0",
    "@types/aws-lambda": "^8.10.22",
    "@types/bcryptjs": "^2.4.2",
    "@types/graphql-depth-limit": "^1.1.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/validator": "^10.9.0",
    "@types/ws": "^6.0.1",
    "bcryptjs": "^2.4.3",
    "env-cmd": "^8.0.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import-node": "^0.0.1",
    "graphql-shield": "^5.3.1",
    "graphql-yoga": "^1.14.10",
    "jsonwebtoken": "^8.5.0",
    "prisma-binding": "^2.3.5",
    "prisma-client-lib": "^1.28.3",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "nodemon": "^1.17.5"
  }
}
